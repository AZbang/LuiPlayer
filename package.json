{
  "name": "YTplayer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "gulp vueify && electron .",
    "build:all": "npm run build:macos && npm run build:linux && npm run build:windows && gulp build",
    "build:macos": "electron-packager . --overwrite --asar --out=dist --ignore='^/dist$' --platform=darwin --arch=x64 --app-bundle-id=com.azbang.sinfonia && cd dist/Keep-darwin-x64 && zip -ryXq9 ../Keep-macos-${npm_package_version}.zip Keep.app",
    "build:linux": "electron-packager . --overwrite --out=dist --ignore='^/dist$' --platform=linux --arch=x64 --app-bundle-id=com.azbang.sinfonia && cd dist/Keep-linux-x64 && zip -ryq9 ../Keep-linux-${npm_package_version}.zip *",
    "build:windows": "electron-packager . --overwrite --asar --out=dist --ignore='^/dist$' --platform=win32 --arch=ia32  --version-string.ProductName=$npm_package_productName && cd dist/Keep-win32-ia32 && zip -ryq9 ../Keep-windows-${npm_package_version}.zip *"
  },
  "devDependencies": {
    "electron": "^1.4.4",
    "electron-packager": "^8.1.0",
    "gulp": "^3.9.1",
    "gulp-vueify": "0.0.3"
  },
  "author": "azbang",
  "dependencies": {
    "vue": "^2.4.2",
    "youtube-dl": "^1.11.1",
    "youtube-search": "^1.0.10",
    "yt-player": "^2.5.2"
  }
}
